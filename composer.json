{
    "name": "webcityro/laralangs",
    "description": "Adding multi languages capability to laravel models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrei Valcu",
            "email": "andreivalcu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "Webcityro\\Laralangs\\": "src/",
            "Webcityro\\Laralangs\\Database\\": "database/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Webcityro\\Laralangs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webcityro\\Laralangs\\LaralangsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0@dev"
    }
}
